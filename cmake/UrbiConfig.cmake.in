@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

if (WIN32)
    set(BOOST_ROOT "${CMAKE_CURRENT_LIST_DIR}/../../..")
    set(BOOST_LIBRARYDIR "${CMAKE_CURRENT_LIST_DIR}/../../../bin")
endif()

add_definitions(-DBOOST_ALL_NO_LIB)
add_definitions(-DBOOST_ALL_DYN_LINK)
set(Boost_USE_DEBUG_RUNTIME OFF)

set(CMAKE_PREFIX_PATH ${CMAKE_PREFIX_PATH} "${CMAKE_CURRENT_LIST_DIR}/../../..")

include(CMakeFindDependencyMacro)

set(URBI_GIT_REVISION "@GIT_REVISION@")

find_dependency(Boost COMPONENTS regex system filesystem thread chrono date_time signals)
find_dependency(Port)
find_dependency(Qijpeg)

include("${CMAKE_CURRENT_LIST_DIR}/UrbiTargets.cmake")
